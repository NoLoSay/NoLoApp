{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@global/*": ["src/global/*"],
      "@assets/*": ["src/assets/*"],
      "@helpers/*": ["src/helpers/*"],
      "@screens/*": ["src/screens/*"]
    }
  }
}

